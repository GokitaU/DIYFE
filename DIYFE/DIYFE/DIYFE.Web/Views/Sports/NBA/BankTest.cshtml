        <div class="span9">
        <h1>Bank Test</h1>

            <table class="table  table-condensed">
                <tr>
                    <th>Number</th>
                    <th>Bet Amount</th>
                    <th>A</th>
                    <th>B</th>
                    <th>C</th>
                    <th>D</th>
                    <th>Required</th>
                    <th>Bank</th>
                    <th>Winnings</th>
                    <th>CashOut</th>
                </tr>
               
            
        @foreach (Sports.Bank bank in Model)
        {
                <tr>
                    <td>@(Model.IndexOf(bank) + 1)</td>
                    <td>@bank.WinningCurrent</td>
                    <td>@bank.ABetAmount</td>
                    <td>@bank.BBetAmount</td>
                    <td>@bank.CBetAmount</td>
                    <td>@bank.DBetAmount</td>
                    <td>@bank.RequiredBank</td>
                    <td>@bank.CurrentBank</td>
                    <td>@bank.WinningTotal</td>
                    <td>@bank.CashOut</td>
                </tr>
        }

        </table>

        </div>
</div>
