@model List<DIYFE.EF.Article>


@if (Model.Count() > 0){
    
    <style>
        .leftPaddingFix
        {
            padding-left:40px;
        }

    </style>
      
    foreach (DIYFE.EF.Article listItem in Model)
    {

    <div class="span12 box-content" style="margin-bottom:10px;">
        <div class="span12">
            <div class="row-fluid">
                <div class="span12">
                    <h4><strong><a href="@listItem.URLLink">@listItem.Name</a></strong></h4>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span2">
                    <a href="#" class="thumbnail">
                    <img src="http://placehold.it/260x180" alt="">
                    </a>
                </div>
                <div class="span9">
                    <p>
                    @listItem.ListItemContent
                    </p>
                    <p><a class="btn" href="@listItem.URLLink">Read more</a></p>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                    <p></p>
                    <p>
                    <i class="icon-user"></i> by <a href="#">@listItem.Author</a>
                    | <i class="icon-calendar"></i> @listItem.CreatedDate.ToShortDateString();
                    | <i class="icon-comment"></i> <a href="#">3 Comments</a>
                    | <i class="icon-share"></i> <a href="#">@listItem.ViewRequests Views</a>
                    | <i class="icon-tags"></i> Tags : <a href="#"><span class="label label-info">Snipp</span></a>
                    <a href="#"><span class="label label-info">Bootstrap</span></a>
                    <a href="#"><span class="label label-info">UI</span></a>
                    <a href="#"><span class="label label-info">growth</span></a>
                    </p>
                </div>
            </div>
        </div>
    </div>
    
    <hr>
   
}   
