            <div id="masterErrorDiv">
            <div class="alert alert-error">
                 <p id="masterErrorText"></p>
            </div>
        </div>
            
      
    <div id="container">
        <header>
            <div id="headerBar"></div>
            <div class="content">
                <div id="logoFlag">
                    <div style="height:270px; background-color:#034769"><!--<p><small>DIYFE: Do It Yourself For Everyone. You do it, build, learn about it, and create it, all because you want too. Sshare your experiences, knowledge and resources with the rest of the world. Thus you do it for yourself for everyone else to build from.</small></p>--></div>
                    <div style="padding-left:30px; background-color:#034769; font-weight:bold;">DIY<br />FE - text based logo-flag thing</div>
                </div>
                <div id="topContent" style="padding-top:3px;">
                    <div class="row">
                        <div class="col-md-3 socialShare">
                            <ul>
						    <li class="facebook"></li>
						    <li class="twitter"></li>
						    <li class="google-plus"></li>
						    <li class="youtube"></li>
						    <li class="foursquare"></li>
					        </ul>
                        </div>
                         <div class="col-md-6">
                            <div id="siteSearch"><input type="text" /> site search </div>
                         </div>
                    </div>
                    
                </div>
                <div id="bottomContent" style="padding-top:10px;">
                  @DIYFEWeb.StaticConfig.TopNavHtml
                 <div id="responsive">
                            <a href="#" class="toggleMenu">MENU</a>
                            <div id="responsive-menu-holder">
                                <div id='responsive-menu'>
                                    <div class='mainMenu'>
                                        <div class="menu"></div>
                                        <div class="utility"></div>
                                    </div>
                                    <div class='subMenu'></div>
                                </div>
                            </div>
                        </div>
                </div>

            </div>
        </header>
        <main>
              @RenderBody()
        </main>
        <br /><br />
        <footer>
            <div class="content">
                <div>FOOTER CONTENT</div>
                <div class="row">
                    <div class="col-md-3"><h4>Categories</h4>
                        <ul>
                            <li><a href="#">Main Nav Items</a> </li>
                            <li><a href="#">Water</a> </li>
                            <li><a href="#">Shelter</a> </li>
                            <li><a href="#">Food</a> </li>
                            <li><a href="#">Communication & Education</a> </li>
                            <li><a href="#">Manufacturing Goods</a> </li>
                        </ul>
                    </div>
                    <div class="col-md-3"><h4>Help Fund Projects</h4>
                        <ul>
                             <li><a href="#">Projects</a></li>
                            <li><a href="#">Products</a></li>
                            <li><a href="#">Research</a></li>
                        </ul>
                    </div>
                    <div class="col-md-3"><h4>Contact</h4>
                        <ul>
                            <li><a href="#">Online Form</a></li>
                            <li><a href="#">We'll promot your project or product</a></li>
                            <li><a href="#">Add Space</a></li>
                            <li><a href="#">email</a></li>
                            <li><a href="#">phone</a></li>
                        </ul>
                    </div>
                    <div class="col-md-3"><h4>View More</h4>
                        <ul>
                             <li><a href="#">Youtube Channel</a></li>
                            <li><a href="#">Kickstarter projects</a></li>
                            <li><a href="#">RSS Feed</a></li>
                            <li><a href="#">Social media share links</a></li>
                        </ul>
                    </div>
                     
                </div>
                <div class="row" style="border-top:1px solid black;">
                    <div class="col-lg-9">
                        privacy policy - copy write - and blablalbal
                    </div>
                </div>
            </div>
        </footer>

    </div>
       @Html.Partial("_LogOnForm")  

    </body>
</html>





