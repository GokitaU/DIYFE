@model List<DIYFEWeb.Models.CustomHtmlLink>

@if (Model.Count > 0){

    <div id="breadcrumb">
        <ul class="breadcrumb">
            @foreach (DIYFEWeb.Models.CustomHtmlLink link in Model)
            {
                if (link == Model.Last())
                {
                     <li class="active">@link.LinkText</li>
                }else{
                    <li>@Html.Raw(link.GenerateHtml) <span class="divider"></span></li>
                }
            }
         </ul>
    </div>
}